<%- include('../parts/html-head') %> <%- include('../parts/navbar') %>

<div class="container">
    <div class="row">
        <div class="col">
            <table class="table">
                <thead>
                    <tr>
                        <th scope="col">#</th>
                        <th scope="col">姓名</th>
                        <th scope="col">信箱</th>
                        <th scope="col">手機</th>
                        <th scope="col">生日</th>
                        <th scope="col">住址</th>                        
                    </tr>
                </thead>
                <tbody>
                    <% for (let i of rows) {%>
                    <tr>
                        <th scope="row"><%= i.sid %></th>
                        <td><%= i.name %></td>
                        <td><%= i.email %></td>
                        <td><%= i.mobile %></td>
                        <td><%= i.birthday %></td>
                        <td><%= i.address %></td>
                    </tr>
                    <% } %>
                </tbody>
            </table>
        </div>
    </div>
</div>

<%- include('../parts/scripts') %> <%- include('../parts/html-foot') %>
